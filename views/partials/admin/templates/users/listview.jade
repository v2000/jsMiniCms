div#menu_wrapper(ng-init='/views/menuAdmin' ng-include src=menu_template_src)
    div#container
    div#form_container
        table.listview
            thead
            th {{myObj['ID']}}
            th {{myObj['Username']}}
            th {{myObj['Email']}}
            th {{myObj['Created']}}
            th {{myObj['Rol']}}
            th {{myObj['Edit']}}
            th {{myObj['Delete']}}

            tbody
            tr(ng-repeat="item in users")
                td.center {{item._id}}
                td {{item.username}}
                td {{item.email}}
                td {{item.date}}
                td {{item.rol}}

                td.center: a(href={{item.edit_url}}) Edit
                td.center: a(href=# data-deleteurl={{item.delete_url}} ng-click=confirmDelete($event)) Delete

            tr
                td(colspan=8 class=center nodata{{myObj['No records to display']}})